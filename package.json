		"eslint": "^8.33.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-svelte": "^2.35.1",
		"helmet": "^7.1.0",
		"http-link-header": "^1.1.3",
		"inquirer": "^9.1.4",
		"jsonwebtoken": "^9.0.0",
		"nodemailer": "^6.9.1",
		"nodemon": "^3.0.1",
		"polka": "^0.5.2",
		"postcss-load-config": "^4.0.1",
		"prettier": "^3.1.0",
		"prettier-plugin-prisma": "^5.0.0",
		"prettier-plugin-svelte": "^3.1.2",
		"prisma": "^5.7.1",
		"sanitize-html": "^2.9.0",
		"svelte": "^4.2.7",
