{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true,
		"typeRoots": ["./types", "./node_modules/@types"]
	},
	"include": [
		"src/app.d.ts"
	],
	"exclude": [
		"src/lib/i18n/messages/**/*.js",
		"src/lib/i18n/messages.js"
	]
}
